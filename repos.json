[
  {
    "kernelSource": {
      "name": "OnePlus7Pro_Nethunter",
      "repo": "https://github.com/LineageOS/android_kernel_oneplus_sm8150",
      "branch": "lineage-18.1",
      "device": "guacamole"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://github.com/kdrag0n/proton-clang.git",
        "branch": "master",
        "name": "clang"
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CROSS_COMPILE": "aarch64-linux-android-",
      "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
      "CC": "clang",
      "CLANG_TRIPLE": "aarch64-linux-gnu-",
      "HOSTCFLAGS": "-fno-stack-protector"
    },
    "extraParams": {
      "NETHUNTER": "1"
    },
    "defconfig": "vendor/sm8150-perf_defconfig",
    "nethunter": {
      "enabled": true,
      "patches": [
        "BT_RFCOMM",
        "INTERNAL_BT",
        "RTL_BT",
        "HID",
        "HID_4",
        "INJECTION",
        "ATH9K_HTC",
        "RTL8188EUS",
        "RTL8812AU",
        "RTL88XXAU",
        "RTL88X2BU",
        "NFS",
        "CAN",
        "NEXMON"
      ]
    }
  }
]
